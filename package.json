{
  "name": "quickstart-deque-appium-webdriverio-demo",
  "version": "1.0.0",
  "description": "Simple js project to install and test an app on devices and execute appium automation together with Deque accessibility scanning",
  "main": "test/specs/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test.android.local.emu": "npx wdio configs/wdio.android.local.emu.conf.ts",
    "test.android.sauce.real.device.eu": "REGION=eu npx wdio configs/wdio.android.sauce.real.conf.ts",
    "test.android.sauce.real.device.us": "npx wdio configs/wdio.android.sauce.real.conf.ts",
    "test.ios.local.sim": "npx wdio configs/wdio.ios.local.sim.conf.ts",
    "test.ios.sauce.real.device.eu": "REGION=eu npx wdio configs/wdio.ios.sauce.real.conf.ts",
    "test.ios.sauce.real.device.us": "npx wdio configs/wdio.ios.sauce.rdc.real.ts",
    "wdio": "wdio run ./wdio.conf.ts"
  },
  "author": "Mobile Team",
  "license": "MIT",
  "devDependencies": {
    "@axe-devtools/appium-plugin": "^2.2.0",
    "@types/mocha": "^10.0.7",
    "@wdio/appium-service": "^8.39.1",
    "@wdio/cli": "^8.39.1",
    "@wdio/globals": "^8.39.1",
    "@wdio/local-runner": "^8.39.1",
    "@wdio/mocha-framework": "^8.39.0",
    "@wdio/runner": "^8.39.1",
    "@wdio/sauce-service": "^8.39.1",
    "@wdio/spec-reporter": "^8.39.0",
    "@wdio/sync": "^7.27.0",
    "@wdio/types": "^8.39.0",
    "appium": "^2.11.1",
    "appium-xcuitest-driver": "^7.23.0",
    "axe-core": "^4.9.1",
    "chai": "^5.1.1",
    "deepmerge": "^4.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.3"
  },
  "keywords": []
}
